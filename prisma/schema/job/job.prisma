model Job {
    id          String    @id @default(dbgenerated("gen_random_uuid()"))
    description String
    company     Company   @relation(fields: [companyId], references: [id], onDelete: Cascade)
    companyId   String
    applicantId String
    Applicant   Applicant @relation(fields: [applicantId], references: [id])

    @@map("job")
}
