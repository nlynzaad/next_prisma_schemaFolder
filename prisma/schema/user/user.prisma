model User {
    id        String    @id @default(dbgenerated("gen_random_uuid()"))
    email     String    @unique
    authId    String    @unique
    authType  AuthType
    createdAt DateTime  @default(now())
    updatedAt DateTime? @updatedAt

    applicant         Applicant?
    companyTeamMember CompanyTeamMember?

    @@map("user")
}

enum AuthType {
    EMAIL_PASSWORD
    GOOGLE
}
